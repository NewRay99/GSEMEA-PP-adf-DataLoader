{
	"name": "LS_FileShare",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"LS_Host": {
				"type": "String"
			},
			"LS_KVS_UserName": {
				"type": "String"
			},
			"LS_KVS_Password": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "FileServer",
		"typeProperties": {
			"host": "@{linkedService().LS_Host}",
			"userId": "@{linkedService().LS_KVS_UserName}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LS_KV_prodkv01",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().LS_KVS_Password",
					"type": "Expression"
				}
			},
			"connectVia": {
				"referenceName": "AutoResolveIntegrationRuntime",
				"type": "IntegrationRuntimeReference"
			}
		}
	}
}